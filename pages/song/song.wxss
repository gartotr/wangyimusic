/* pages/song/song.wxss */
.songContainer {
  background-image:url('data:image/jpeg;base64,');

  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  background-repeat:no-repeat;
  background-size:100% 100%;
}
.songContainer .keng{
  width:60rpx;
  height:60rpx;
  background: white;
  border-radius: 50%;
  z-index: 10;
}
.songContainer .needle {
  width: 192rpx;
  height: 274rpx;
  position: relative;
  left:58rpx;
  top:-36rpx;
  z-index: 9;

  transform-origin: 40rpx 0;
  transition: transform 400ms;
  transform: rotateZ(-20deg);
}
/* 唱片动画 */
@keyframes run {
  from{
    transform: rotateZ(0);
  }
  to{
    transform: rotateZ(360deg);
  }
}
.songContainer.isplaying .needle{
  transform:rotateZ(0deg);
}

.songContainer .discContainer{
  width:598rpx;
  height:598rpx;
  position: relative;
  top:-180rpx;
  animation: run 20s infinite linear;
  animation-play-state: paused;
}

.songContainer.isplaying .discContainer{
  animation-play-state: running;
}


.songContainer .discContainer .disc{
  position: relative;
  width:100%;
  height:100%;
  z-index: 6;
}
.songContainer .discContainer .bigImg{
position: absolute;
top: 0;
right: 0;
bottom: 0;
left: 0;
margin: auto;
width:370rpx;
height:370rpx;
z-index: 5;
}

/* musicControl */
.musicControl {
  position: absolute;
  bottom: 40rpx;
  left: 0;
  width: 100%;
  display: flex;
  justify-content: space-between;
  border-top: 1rpx solid #fff;
}

.musicControl text {
  width: 20%;
  text-align: center;
  font-size: 50rpx;
  color: #fff;
  height: 120rpx;
  line-height: 120rpx;
}
.musicControl text.big {
  font-size: 80rpx;
}

.audioContainer{
  position: absolute;
  bottom:200rpx;
  display:flex;
  width:640rpx;
  height:80rpx;
  line-height:80rpx;
}

.audioContainer .barContainer{
  width:450rpx;
  height:4rpx;
  background:rgba(0, 0, 0, 0.4);
  margin:auto;
}

.audioContainer .barContainer .audio-current-time{
  position: relative;
  width:50%;
  height:4rpx;
  background:red;
}

.audioContainer .barContainer .audio-current-time .audio-time-point{
  position: absolute;
  top:-4rpx;
  right:-6rpx;
  width:12rpx;
  height:12rpx;
  border-radius: 50%;
  background:white;
}

.time-outer{
  display:flex;
  align-items: center;

}
.progressBar-outer{
  width:450rpx;
  height:4rpx;
  background: rgba(0, 0, 0, 0.4);
  margin:0 20rpx;
}
.progressBar-outer .progressBar-inner{
  position: relative;
  width:0%;
  height:4rpx;
  background: red;
}
.progressBar-outer .progressBar-inner .box{
  position: absolute;
  border-radius: 50%;
  width:12rpx;
  height:12rpx;
  background: #fff;
  top: -4rpx;
  right:-6rpx;
}